{
  "devDependencies": {
    "@playwright/test": "^1.36.2"
  },
  "scripts": {
    "test": "playwright test -j 1 --retries 0 --headed --project=chromium",
    "reg": "playwright test --project=chromium",
    "report": "playwright show-report playwright-report",
    "ui": "playwright test --ui-port 0",
    "trace": "playwright show-trace --port 0",
    "allure": "npx allure generate ./allure-results --clean && npx allure open ./allure-report"
  },
  "dependencies": {
    "allure-commandline": "^2.23.1",
    "allure-playwright": "^2.4.0",
    "dotenv": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-playwright": "^0.15.3"
  }
}
